{
  "manifest_version": 2,
  "name": "Random Quote Generator",
  "version": "1.0",
  "description": "Generate random quotes in your new tab.",
  "permissions": ["activeTab"],
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Random Quote Generator",
    "default_popup": "popup.html"
  }
}
